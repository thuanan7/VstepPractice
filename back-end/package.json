{
  "name": "crawler-back-end",
  "version": "1.0.0",
  "description": "crawl data",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "c": "rm -rf node_modules && rm -rf yarn.lock && rm -rf dist",
    "ci": "yarn c && yarn",
    "update-db": "node scripts/beforeRunMigration.cjs && npx sequelize-cli db:migrate && yarn seed",
    "add-migrate": "bash scripts/add-model.bash",
    "seed": "sequelize db:seed:all",
    "build": "npx tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts"
  },
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.8",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "chalk": "^5.3.0",
    "cheerio": "^1.0.0-rc.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "joi": "^17.13.0",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "microdiff": "^1.4.0",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.45",
    "mssql": "^10.0.2",
    "node-cron": "^3.0.3",
    "ora": "^8.0.1",
    "puppeteer": "^22.5.0",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "rootpath": "^0.1.2",
    "sequelize": "^6.37.3",
    "sequelize-cli": "^6.6.2",
    "tedious": "18.2.0",
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/js-yaml": "^4.0.9",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.12.7",
    "@types/node-cron": "^3.0.11",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.3",
    "typescript": "^5.4.5"
  }
}
