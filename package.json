{
  "name": "tkpm-2024",
  "private": true,
  "version": "0.0.0",
  "main": "index.js",
  "scripts": {
    "c": "rm -rf node_modules && rm -rf dist && rm -rf pnpm-lock.yaml && rm -rf yarn.lock",
    "ci": "pnpm run c && pnpm i",
    "seed": "sequelize db:seed:all --config ./back-end/db/config/config.json --seeders-path back-end/db/seeders",
    "migrate": "npx sequelize-cli db:migrate --config back-end/db/config/config.json",
    "net-api": "dotnet run --project ./back-end/netcore/VstepPractice.API",
    "node-api": "npx nodemon back-end/node/index.js",
    "web": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "backup-db": "pg_dump -U postgres -d tkpm2024 -F c -f ./doc/tkpm2024_db_backup_$(date +\"%Y%m%d_%H%M%S\").backup",
    "rb-dump": "pg_restore -U postgres -d tkpm2024test ./doc/mytestdb_backup.dump",
    "dev": "concurrently \"pnpm node-api\" \"pnpm web\"",
    "msr": "yarn migrate && yarn seed"
  },
